<template id="linkn-links">
  <linkn-linkgroup :links="[
                       {% for link in links %}
                        {% if link.fields.Title and link.fields.Link %}
                         {
                           label: '{{ link.fields.Title }}',
                           link: '//{{ link.fields.Link }}',
                           redirect: mode !== 'grid',
                           placeholder: '{{ path.assets }}/{{ settings.placeholder }}'
                         },
                        {% endif %}
                       {% endfor %}
                     ]"
                     :class="[ '-' + mode ]"></linkn-linkgroup>
</template>
