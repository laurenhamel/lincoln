<template id="lincoln-links">
  <lincoln-linkgroup :links="[
                       {% for link in links %}
                        {% if link.fields.Title and link.fields.Link %}
                         {
                           label: '{{ link.fields.Title }}',
                           link: '//{{ link.fields.Link }}',
                           redirect: mode !== '{{ settings.layout }}',
                           placeholder: '{{ path.assets }}/{{ settings.placeholder }}'
                         },
                        {% endif %}
                       {% endfor %}
                     ]"
                     :class="[ '-' + mode ]"></lincoln-linkgroup>
</template>
